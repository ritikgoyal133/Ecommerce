<%# <%= javascript_include_tag "sign_up", "data-turbolinks-track" => true  %>
    <div class="container">
      <div class="row">
        <div class="col-md-offset-4 col-md-4 a">
          <h2 class="text-center heading">Sign Up</h2>
            <%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
              <div class="field form-group">
                <%= f.label :username %><br />
                <%= f.text_field :username, autofocus: true, class: 'form-control'%>
              </div>
              <div class="field form-group">
                <%= f.label :address %><br />
                <%= f.text_field :address, autofocus: true , class: 'form-control'%>
              </div>
              <div class="field form-group">
                <%= f.label :email %><br />
                <%= f.email_field :email, autofocus: true, autocomplete: "email", class: 'form-control'%>
              </div>
              <div class="field form-group">
                <%= f.label :password %>
                <%= f.password_field :password, autocomplete: "new-password" , class: 'form-control'%>
              </div>
              <div class="field form-group">
                <%= f.label :password_confirmation %><br />
                <%= f.password_field :password_confirmation, autocomplete: "new-password" , class: 'form-control'%>
              </div>
              <div>
              <%= f.submit "Sign up", class: 'btn btn-block b' %>
                </div><br>
            <% end %>
              <%= render "users/shared/links" %>
        </div>
        
    </div>
</div>
<script>
$(document).ready(function(){
  var home = window.home();
  home.validate();
})
</script>
     